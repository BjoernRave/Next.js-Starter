{
  "compilerOptions": {
    "sourceMap": true,
    "outDir": "dist",
    "strict": false,
    "lib": ["esnext"],
    "esModuleInterop": true,
    "skipLibCheck": true,
    "jsx": "react",
    "resolveJsonModule": true,
    "downlevelIteration": true,
    "baseUrl": "./",
    "paths": {
      "components/*": ["./components/*"],
      "lib/*": ["./lib/*"],
      "generated": ["./generated"]
    }
  },
  "exclude": ["node_modules", ".next"]
}
