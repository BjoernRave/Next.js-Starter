{
  "name": "nextjs-starter",
  "version": "1.0.0",
  "description": "",
  "main": "pages/index.tsx",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "analyze": "ANALYZE=true yarn build",
    "generate:back": "graphql-codegen --config codegen.yml",
    "generate": "yarn generate:prisma && yarn generate:nexus",
    "generate:prisma": "prisma generate",
    "generate:nexus": "ts-node --project ./tsconfig.prisma.json --transpile-only ./prisma/schema.ts",
    "migrate": "pg_ctl -D /usr/local/var/postgresql@11 restart  &&  prisma migrate --early-access-feature",
    "migrate:save": "prisma migrate save --experimental",
    "migrate:up": "prisma migrate up --experimental"
  },
  "dependencies": {
    "@nexus/schema": "0.19.1",
    "@prisma/client": "2.12.0",
    "@urql/exchange-graphcache": "^3.2.0",
    "apollo-server-micro": "^2.19.0",
    "framer-motion": "^2.9.4",
    "graphql": "^15.4.0",
    "graphql-middleware": "^4.0.2",
    "graphql-shield": "^7.4.2",
    "micro-cors": "^0.1.1",
    "next": "^10.0.3",
    "next-urql": "^2.1.1",
    "nexus-plugin-prisma": "0.25.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-ssr-prepass": "^1.2.1",
    "styled-components": "^5.2.1",
    "urql": "^1.11.3"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.19.3",
    "@graphql-codegen/introspection": "^1.18.1",
    "@graphql-codegen/typescript": "^1.18.1",
    "@graphql-codegen/typescript-operations": "^1.17.12",
    "@graphql-codegen/typescript-urql": "^2.0.3",
    "@next/bundle-analyzer": "^10.0.3",
    "@prisma/cli": "2.12.1",
    "@types/node": "^14.14.10",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/styled-components": "^5.1.4",
    "autoprefixer": "10.0.4",
    "babel-plugin-graphql-tag": "^3.1.0",
    "babel-plugin-inline-import": "^3.0.0",
    "babel-plugin-styled-components": "^1.12.0",
    "dotenv": "^8.2.0",
    "postcss": "8.1.10",
    "prettier": "^2.2.1",
    "prettier-plugin-organize-imports": "^1.1.1",
    "tailwindcss": "^2.0.1",
    "typescript": "^4.1.2"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "babel": {
    "presets": [
      "next/babel"
    ],
    "plugins": [
      "graphql-tag",
      "inline-import",
      [
        "styled-components",
        {
          "ssr": true
        }
      ]
    ]
  }
}
